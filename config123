apiVersion: build.openshift.io/v1
kind: BuildConfig
metadata:
  name: superset-test
  namespace: superset
spec:
  source: superset-test
    type: Binary
  strategy:
    type: Docker
    dockerStrategy:
      dockerfilePath: Dockerfile
  output:
    to:
      kind: ImageStreamTag
      name: superset-test:latest
  triggers:
    - type: ConfigChange
    - type: ImageChange
